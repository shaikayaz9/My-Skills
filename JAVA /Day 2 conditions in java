import java.util.*;

public class DAY1{
    public static void main(String args[]){
        Scanner sc = new Scanner(System.in);
        int age = sc.nextInt();
        
        if(age<18){
            System.out.println("not drivr");
        }else if(age>60){
            System.out.println("pls drive carefully");
        }else{
            System.out.println("drive buddy");
        }
    }
}


